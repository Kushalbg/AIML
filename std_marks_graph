import pandas as pd
import matplotlib.pyplot as plt
mrk_sci=[30,25,24,12,26,20]
mrk_soc=[15,20,25,19,6,30]
mrk_eng=[9,11,12,25,4,30]
df_marks=pd.DataFrame({'sci':mrk_sci,
                       'soc':mrk_soc,
                       'eng':mrk_eng})

print(df_marks)
df_marks.plot.bar()
df_marks.plot.density()
plt.show()
df_marks.boxplot()
plt.show()
